file = {:home/rais/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Stowers, Lentink - 2015 - Folding in and out passive morphing in flapping wings.pdf:pdf},
isbn = {1748-3190},
issn = {17483190},
journal = {Bioinspiration {\&} biomimetics},
keywords = {animal fl ight,animal flight,article is available online,bioinspired,fl apping wings,flapping wings,supplementary material for this,sweep,sweep{\_}flapping,underactuated,wing morphing},
mendeley-tags = {sweep{\_}flapping},
