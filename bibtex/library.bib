pages = {1177--1188},
title = {{Dynamics and Performance of Tailless Micro Aerial Vehicle with Flexible Articulated Wings}},
url = {http://arc.aiaa.org/doi/10.2514/1.J051447},
volume = {50},
year = {2012}
}
